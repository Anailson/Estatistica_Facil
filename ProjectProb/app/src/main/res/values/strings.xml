    <!-- XML and HTML Symbol -->
    <string name="sym_avg_arithmetic">x&#772; = &#8721;(&#967;&#161;) / &#8721;(n)</string>
    <string name="sym_avg_arithmetic_pound">x&#772; = &#8721;(&#967;&#161; * &#402;&#161;) / &#8721;(&#402;&#161;)</string>
    <string name="sym_avg_geometric">x&#772; = &#8719;(&#967;&#161;) ^ (1 / n)</string>
    <string name="sym_avg_geometric_pound">x&#772; = [&#8719; (&#967;&#161; ^ &#402;&#161;)] ^ [1 / &#8721;(&#402;&#161;)]</string>
    <string name="sym_avg_weighted">x&#772; = n / &#8721;(1 / &#967;&#161;)</string>
    <string name="sym_avg_weighted_pound">x&#772; = &#8721;(&#402;&#161;) / &#8721;(&#402;&#161; / &#967;&#161;)</string>
    <string name="sym_avg_quadratic">x&#772; = &#8730;(&#8721; (&#967;&#161; ^ 2))</string>
    <string name="sym_avg_quadratic_pound">x&#772; = &#8730;[&#8721; (&#967;&#161; ^ 2 * &#402;&#161;)]</string>
    <string name="sym_kurtosis">K = (Q&#8323; - Q&#8321;) / 2(D&#8329; - D&#8321;)</string>
    <string name="sym_asymmetry_pearson_st">A = (x&#772; - Mo) / S</string>
    <string name="sym_asymmetry_pearson_nd">A = 3(x&#772; - Md) / S</string>
    <string name="sym_asymmetry_quart">A = (Q&#8323; + Q&#8321; - 2Q&#8322;) / (Q&#8323; - Q&#8321;)</string>
    <string name="sym_asymmetry_percent">A = (P&#8329;&#8320; + P&#8321;&#8320; - 2P&#8325;&#8320;) / (P&#8329;&#8320; - P&#8321;&#8320;)</string>
    <string name="sym_standard_deviation">E = &#8730;{E(&#967;&#161; ^ 2) - [E(&#967;&#161;)] ^ 2}</string>
    <string name="sym_fst_quart">Q&#8321; = (n + 1) / 4</string>
    <string name="sym_snd_quart">Q&#8322; = (n + 1) / 2</string>
    <string name="sym_trd_quart">Q&#8323; = 3 * (n + 1) / 4</string>
    <string name="sym_fst_tenth">D&#8321; = (n + 1) / 10</string>
    <string name="sym_nth_tenth">D&#8329; = 9 * (n + 1) / 10</string>

    <string name="sym_interval_avg">Q&#8323; = 3 * (n + 1) / 4</string>
    <string name="sym_interval_prop">D&#8321; = (n + 1) / 10</string>
    <string name="sym_interval_var">D&#8329; = 9 * (n + 1) / 10</string>

